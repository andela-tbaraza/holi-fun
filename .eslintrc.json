{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "airbnb",
    "rules": {
      "comma-dangle": 0,
      "vars-on-top": 0,
      "import/no-extraneous-dependencies": ["error", {"devDependencies": true } ],
      "no-param-reassign": 0,
      "arrow-parens": ["error", "as-needed"]
    }
}
